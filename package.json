{
  "name": "lazy_days_playground",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "pnpm typegen && pnpm build:react && pnpm build:server",
    "build:react": "react-router build",
    "build:server": "tsc --project tsconfig.server.json",
    "build:without-typegen": "pnpm build:react && pnpm build:server",
    "dev": "node --env-file=.env.local.dev ./server/index.ts",
    "fresh-install": "rm -rf node_modules && pnpm store prune && rm -rf .react-router && rm -rf coverage && rm -rf dist && rm pnpm-lock.yaml && pnpm update && pnpm test:coverage && pnpm typegen && pnpm lint && pnpm typecheck:without-typegen && pnpm build:without-typegen",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0 --rule \"no-console: ['error', { allow: ['error', 'info', 'warn'] }]\" --rule \"react-hooks/exhaustive-deps: off\"",
    "lint:fix": "pnpm lint --fix",
    "obsolete": "knip",
    "start": "node --env-file=.env.local.prod ./dist/fastify/server/index.js",
    "start:clean": "pnpm fresh-install && pnpm start",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "typecheck": "pnpm typegen && tsc --noEmit",
    "typecheck:without-typegen": "tsc --noEmit",
    "typegen": "pnpm typegen:client && pnpm typegen:server && pnpm typegen:db",
    "typegen:db": "node node_modules/@gel/generate/dist/cli.js interfaces --dsn \"gel://admin:password@host.docker.internal:5656/main?tls_security=insecure\" --file shared/types/generated/db/database.type.ts",
    "typegen:client": "react-router typegen",
    "typegen:server": "VITE_APP_TYPE_GENERATOR_MODE=type_generator node --env-file=.env.local.dev ./server/index.ts"
  },
  "dependencies": {
    "@fastify/cookie": "^11.0.2",
    "@fastify/helmet": "^13.0.2",
    "@fastify/rate-limit": "^10.3.0",
    "@fastify/static": "^8.3.0",
    "@gel/auth-core": "^0.3.1",
    "@hookform/resolvers": "^5.2.2",
    "@iconify/react": "^6.0.2",
    "@react-router/node": "^7.11.0",
    "@react-router/serve": "^7.11.0",
    "@tanstack/react-query": "^5.90.12",
    "axios": "^1.13.2",
    "classnames": "^2.5.1",
    "dayjs": "^1.11.19",
    "fastify": "^5.6.2",
    "fastify-zod-openapi": "^5.5.0",
    "gel": "^2.2.0",
    "get-port": "^7.1.0",
    "html-tags": "^5.1.0",
    "iconify-icon": "^3.0.2",
    "isbot": "^5.1.32",
    "lodash": "^4.17.21",
    "maskdata": "^1.3.4",
    "pino-http": "^11.0.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-hook-form": "^7.68.0",
    "react-router": "^7.11.0",
    "uuid": "^13.0.0",
    "zod": "^4.2.1",
    "zustand": "^5.0.9",
    "zustand-x": "^6.2.1"
  },
  "devDependencies": {
    "@babel/plugin-transform-explicit-resource-management": "^7.28.0",
    "@babel/preset-typescript": "^7.28.5",
    "@eslint/js": "^9.39.2",
    "@fastify/express": "^4.0.2",
    "@fastify/swagger": "^9.6.1",
    "@fastify/swagger-ui": "^5.2.3",
    "@gel/generate": "^0.7.1",
    "@react-router/dev": "^7.11.0",
    "@redux-devtools/extension": "^3.3.0",
    "@tanstack/eslint-plugin-query": "^5.91.2",
    "@tanstack/react-query-devtools": "^5.91.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@total-typescript/ts-reset": "^0.6.1",
    "@types/express": "^5.0.6",
    "@types/lodash": "^4.17.21",
    "@types/node": "^24.10.4",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/parser": "^8.50.0",
    "@vitest/coverage-v8": "^4.0.16",
    "babel-plugin-react-compiler": "^1.0.0",
    "eslint": "^9.39.2",
    "eslint-plugin-css-modules": "^2.12.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.26",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-sonarjs": "^3.0.5",
    "globals": "^16.5.0",
    "jiti": "^2.6.1",
    "jsdom": "^27.3.0",
    "knip": "^5.75.2",
    "openapi-types": "^12.1.3",
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.3",
    "react-router-devtools": "^6.0.0",
    "sass": "^1.97.0",
    "swagger-typescript-api": "^13.2.16",
    "type-fest": "^5.3.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.50.0",
    "vite": "^7.3.0",
    "vite-plugin-babel": "^1.3.2",
    "vite-plugin-checker": "^0.12.0",
    "vite-tsconfig-paths": "^6.0.3",
    "vitest": "^4.0.16"
  },
  "engines": {
    "node": ">=24.0.0"
  },
  "packageManager": "pnpm@10.26.0",
  "pnpm": {
    "ignoredBuiltDependencies": [
      "@parcel/watcher",
      "esbuild",
      "rollup"
    ],
    "patchedDependencies": {
      "react-router": "patches/react-router.patch"
    }
  }
}
