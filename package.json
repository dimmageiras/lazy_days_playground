{
  "name": "lazy_days_playground",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "pnpm typegen && pnpm build:react && pnpm build:server",
    "build:react": "react-router build",
    "build:server": "tsc --project tsconfig.server.json",
    "dev": "node --env-file=.env.local.dev ./server/index.ts",
    "gel:generate": "node node_modules/@gel/generate/dist/cli.js interfaces --dsn \"gel://admin:password@host.docker.internal:5656/main?tls_security=insecure\"",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0 --rule \"no-console: ['error', { allow: ['error', 'info', 'warn'] }]\" --rule \"react-hooks/exhaustive-deps: off\"",
    "lint:fix": "pnpm lint --fix",
    "start": "node --env-file=.env.local.prod ./dist/fastify/server/index.js",
    "typecheck": "pnpm typegen && tsc --noEmit",
    "typegen": "react-router typegen"
  },
  "dependencies": {
    "@fastify/auth": "5.0.2",
    "@fastify/cookie": "11.0.2",
    "@fastify/jwt": "10.0.0",
    "@fastify/swagger": "9.5.1",
    "@fastify/swagger-ui": "5.2.3",
    "@hookform/resolvers": "5.2.1",
    "@iconify/react": "6.0.1",
    "@mcansh/remix-fastify": "4.1.1",
    "@react-router/node": "7.8.2",
    "@react-router/serve": "7.8.2",
    "@tanstack/react-query": "5.86.0",
    "axios": "1.11.0",
    "bcrypt": "6.0.0",
    "classnames": "2.5.1",
    "dayjs": "1.11.18",
    "fastify": "5.5.0",
    "gel": "2.1.1",
    "get-port": "7.1.0",
    "html-tags": "5.1.0",
    "iconify-icon": "3.0.0",
    "isbot": "5.1.30",
    "lodash": "4.17.21",
    "pino-http": "10.5.0",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-hook-form": "7.62.0",
    "react-router": "7.8.2",
    "uuid": "11.1.0",
    "zod": "4.1.5",
    "zustand": "5.0.8",
    "zustand-x": "6.1.2"
  },
  "devDependencies": {
    "@eslint/js": "9.34.0",
    "@gel/generate": "0.6.4",
    "@react-router/dev": "7.8.2",
    "@redux-devtools/extension": "3.3.0",
    "@tanstack/eslint-plugin-query": "5.86.0",
    "@tanstack/react-query-devtools": "5.86.0",
    "@total-typescript/ts-reset": "0.6.1",
    "@types/bcrypt": "6.0.0",
    "@types/lodash": "4.17.20",
    "@types/node": "24.3.1",
    "@types/react": "19.1.12",
    "@types/react-dom": "19.1.9",
    "@typescript-eslint/parser": "8.42.0",
    "@typescript-eslint/utils": "8.42.0",
    "eslint": "9.34.0",
    "eslint-plugin-css-modules": "2.12.0",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-react-refresh": "0.4.20",
    "eslint-plugin-security": "3.0.1",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "globals": "16.3.0",
    "jiti": "2.5.1",
