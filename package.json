{
  "name": "lazy_days_playground",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "pnpm typegen && pnpm build:react && pnpm build:server",
    "build:react": "react-router build",
    "build:server": "tsc --project tsconfig.server.json",
    "dev": "node --env-file=.env.local.dev ./server/index.ts",
    "gel:generate": "node node_modules/@gel/generate/dist/cli.js interfaces --dsn \"gel://admin:password@host.docker.internal:5656/main?tls_security=insecure\"",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0 --rule \"no-console: ['error', { allow: ['error', 'info', 'warn'] }]\" --rule \"react-hooks/exhaustive-deps: off\"",
    "lint:fix": "pnpm lint --fix",
    "obsolete": "knip",
    "start": "node --env-file=.env.local.prod ./dist/fastify/server/index.js",
    "typecheck": "pnpm typegen && tsc --noEmit",
    "typegen": "pnpm typegen:client && pnpm typegen:server",
    "typegen:client": "react-router typegen",
    "typegen:server": "VITE_APP_TYPE_GENERATOR_MODE=type_generator node --env-file=.env.local.dev ./server/index.ts"
  },
  "dependencies": {
    "@fastify/cookie": "11.0.2",
    "@fastify/rate-limit": "10.3.0",
    "@fastify/swagger": "9.5.2",
    "@fastify/swagger-ui": "5.2.3",
    "@gel/auth-core": "0.3.1",
    "@hookform/resolvers": "5.2.2",
    "@iconify/react": "6.0.2",
    "@mcansh/remix-fastify": "4.1.2",
    "@react-router/node": "7.9.3",
    "@react-router/serve": "7.9.3",
    "@tanstack/react-query": "5.90.2",
    "axios": "1.12.2",
    "classnames": "2.5.1",
    "dayjs": "1.11.18",
    "fastify": "5.6.1",
    "fastify-zod-openapi": "5.5.0",
    "gel": "2.1.1",
    "get-port": "7.1.0",
    "html-tags": "5.1.0",
    "iconify-icon": "3.0.1",
    "isbot": "5.1.31",
    "lodash": "4.17.21",
    "pino-http": "10.5.0",
    "react": "19.2.0",
    "react-content-loader": "7.1.1",
    "react-dom": "19.2.0",
    "react-hook-form": "7.63.0",
    "react-router": "7.9.3",
    "uuid": "13.0.0",
    "zod": "4.1.11",
    "zustand": "5.0.8",
    "zustand-x": "6.1.2"
  },
  "devDependencies": {
    "@eslint/js": "9.36.0",
    "@gel/generate": "0.6.4",
    "@react-router/dev": "7.9.3",
    "@redux-devtools/extension": "3.3.0",
    "@tanstack/eslint-plugin-query": "5.91.0",
    "@tanstack/react-query-devtools": "5.90.2",
    "@total-typescript/ts-reset": "0.6.1",
    "@types/lodash": "4.17.20",
    "@types/node": "24.6.2",
    "@types/react": "19.2.0",
    "@types/react-dom": "19.2.0",
    "@typescript-eslint/parser": "8.45.0",
    "eslint": "9.36.0",
    "eslint-plugin-css-modules": "2.12.0",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-react-refresh": "0.4.23",
    "eslint-plugin-security": "3.0.1",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "globals": "16.4.0",
    "jiti": "2.6.1",
    "knip": "5.64.1",
