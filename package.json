    "react-router": "7.13.0",
    "uuid": "13.0.0",
    "zod": "4.3.6",
    "zustand": "5.0.11",
    "zustand-x": "6.2.1"
  },
  "devDependencies": {
    "@babel/plugin-transform-explicit-resource-management": "7.28.6",
    "@babel/preset-typescript": "7.28.5",
    "@eslint/js": "9.39.2",
    "@fastify/express": "4.0.4",
    "@fastify/swagger": "9.7.0",
    "@fastify/swagger-ui": "5.2.5",
    "@gel/generate": "0.7.1",
