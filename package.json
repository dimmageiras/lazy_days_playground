    "@total-typescript/ts-reset": "0.6.1",
    "@types/express": "5.0.6",
    "@types/lodash": "4.17.21",
    "@types/node": "24.10.1",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "@typescript-eslint/parser": "8.48.1",
    "babel-plugin-react-compiler": "1.0.0",
    "eslint": "9.39.1",
